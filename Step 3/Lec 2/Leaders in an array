import java.util.*;

class Solution {
    public List<Integer> leaders(int[] nums) {
        List<Integer> result = new ArrayList<>();
        
        int maxSoFar = Integer.MIN_VALUE;

        for (int i = nums.length - 1; i >= 0; i--) {
            if (nums[i] > maxSoFar) {
                result.add(nums[i]);
                maxSoFar = nums[i];
            }
        }

        Collections.reverse(result);
        return result;
    }
}
